  <object name="emas.app.productsfolder" meta_type="Dexterity FTI" />
  <object name="emas.app.orderfolder" meta_type="Dexterity FTI" />
  <object name="emas.app.orderitem" meta_type="Dexterity FTI" />
  <object name="emas.app.order" meta_type="Dexterity FTI" />
  <object name="emas.app.service" meta_type="Dexterity FTI" />
  <object name="emas.app.product" meta_type="Dexterity FTI" />
</object>
